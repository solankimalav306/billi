      <Button fx:id="Customers" layoutX="46.0" layoutY="452.0" mnemonicParsing="false" onAction="#handleCustomersClick" prefHeight="54.0" prefWidth="157.0" style="-fx-background-radius: 12; -fx-opacity: 0;" />
